{
  "expo": {
    "name": "daily-diet",
    "slug": "daily-diet",
    "newArchEnabled": true,
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/system/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/system/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/system/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/system/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
